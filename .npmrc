# Force npm to run node-gyp with platform=linux architecture=x64
force=true
node_gyp=node-gyp
# Configure node-pre-gyp to build for Linux x64
target_platform=linux
target_arch=x64
# Optimize for Vercel platform
target_libc=glibc
legacy-peer-deps=true
# Ensure dependencies are installed properly
resolution-mode=highest 